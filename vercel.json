{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/vite"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)\\.(?:js|css|svg|png|jpg|jpeg|gif|ico)$",
            "dest": "/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}